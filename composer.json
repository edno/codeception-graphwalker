{
  "name": "edno/codeception-graphwalker",
  "description": "Codeception extension for model based testing",
  "keywords": ["codeception", "codeception-extension", "mbt", "model-based-testing", "graphml"],
  "homepage": "https://github.com/edno/codeception-graphwalker",
  "license": "MIT",
  "authors": [
      {
        "name": "Gregory Heitz",
        "email": "edno@edno.io"
      }
  ],
  "require": {
    "php": ">=7.0",
    "codeception/codeception": ">=2.4.3 | <3.0",
    "clue/graph": "^0.9",
    "edno/graphyed": "*"
  },
  "require-dev": {
    "graphp/algorithms": "^0.8",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
	     "edno\\Codeception\\": "src/"
    }
  },
  "suggest": {
    "graphp/algorithms": "Compatible set of graph algorithms for Graphwalker"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
